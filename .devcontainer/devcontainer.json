{
	"name": "Node.js",
	"image": "mcr.microsoft.com/devcontainers/javascript-node:dev-24-bullseye",
	"forwardPorts": [
		3000
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"dsznajder.es7-react-js-snippets"
			]
		}
	},
	"postCreateCommand": "npm install",
	"remoteUser": "node"
}